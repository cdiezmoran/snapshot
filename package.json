{
  "name": "snapshotjs",
  "version": "0.0.0",
  "main": "reactviews/index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "supervisor ./bin/www",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:js && npm run build-css",
    "watch": "npm run watch:html & npm run watch:js & npm run watch-css",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp index.html build/",
    "watch:html": "npm run build:html && chokidar index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:js": "npm run build:js:unminified && npm run build:js:minified",
    "watch:js": "npm run watch:js:unminified & npm run watch:js:minified",
    "clean:js": "npm run clean:js:unminified && npm run clean:js:minified",
    "build:js:unminified": "npm run clean:js:unminified && webpack",
    "watch:js:unminified": "npm run build:js:unminified && webpack --watch",
    "clean:js:unminified": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "build:js:minified": "npm run clean:js:minified && webpack --minify",
    "watch:js:minified": "npm run build:js:minified && webpack --watch --minify",
    "clean:js:minified": "rm -f build/$npm_package_name.$npm_package_version.min.js build/$npm_package_name.$npm_package_version.min.js.map",
    "build-css": "node-sass --include-path views/scss views/scss/main.scss  build/main.css",
    "watch-css": "npm run build-css && chokidar views/scss/main.scss -c 'npm run build-css'"
  },
  "dependencies": {
    "async": "^2.0.1",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.3",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "es6-promise": "^4.1.0",
    "express": "~4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "markdown": "^0.5.0",
    "material-ui": "^0.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.5.10",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "~2.3.0",
    "stylus": "^0.54.5",
    "swig": "^1.4.2",
    "validator": "^5.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chokidar-cli": "^1.2.0",
    "node-sass": "^3.10.1",
    "sass": "^0.5.0",
    "should": "^11.1.0",
    "supervisor": "^0.12.0",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.2"
  },
  "private": true
}
